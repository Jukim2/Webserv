server {
	listen       8888;
	server_name  localhost;
	
	location ^ / {
		root   html;
		index  index.html index.htm;
		index ;
	}

	allow_methods GET POST ;

	location ^ /directory {

		allow_methods POST DELETE;

		alias   YoupiBanane;
		index  youpi.bad_extension;

		error_page   404 /404.html;
	}

	location ^ /auto {
		allow_methods GET;

		root html;
		autoindex on;
	}

	location $ .php {
		cgi_path cgi-bin/index.php;
	}

	location $ .py {
		cgi_path cgi-bin/mycgi.py;
	}

	location $ .bla {
		cgi_path cgi_tester;
	}

	location ^ /google {
		proxy_pass http://www.google.com;
	}

	error_page   500 502 503 504  /50x.html;
	location = /50x.html {
		root   html;
	}
}
